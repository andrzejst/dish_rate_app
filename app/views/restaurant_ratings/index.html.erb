<h1>Listing restaurant_ratings</h1>

<table>
  <thead>
    <tr>
      <th>Rate value</th>
      <th>User</th>
      <th>Restaurant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restaurant_ratings.each do |restaurant_rating| %>
      <tr>
        <td><%= restaurant_rating.rate_value %></td>
        <td><%= restaurant_rating.user_id %></td>
        <td><%= restaurant_rating.restaurant_id %></td>
        <td><%= link_to 'Show', restaurant_rating %></td>
        <td><%= link_to 'Edit', edit_restaurant_rating_path(restaurant_rating) %></td>
        <td><%= link_to 'Destroy', restaurant_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Restaurant rating', new_restaurant_rating_path %>
