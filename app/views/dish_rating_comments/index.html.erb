<h1>Listing dish_rating_comments</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th>Dish rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dish_rating_comments.each do |dish_rating_comment| %>
      <tr>
        <td><%= dish_rating_comment.content %></td>
        <td><%= dish_rating_comment.user_id %></td>
        <td><%= dish_rating_comment.dish_rating_id %></td>
        <td><%= link_to 'Show', dish_rating_comment %></td>
        <td><%= link_to 'Edit', edit_dish_rating_comment_path(dish_rating_comment) %></td>
        <td><%= link_to 'Destroy', dish_rating_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dish rating comment', new_dish_rating_comment_path %>
